<?xml version="1.0" encoding="utf-16"?>
<interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.thermo.com/informatics/xmlns/interface/1.0">
  <forms>
    <form name="Page_General">
      <caption>Allgemein</caption>
      <extendedfunctions />
      <controls>
        <EntityImage name="EntityImage1" />
        <prompt name="RuleId" />
        <line name="Line" />
        <prompt name="RuleName">
          <caption>Name</caption>
        </prompt>
        <prompt name="Description">
          <caption>Beschreibung</caption>
        </prompt>
        <line name="Line1" />
        <prompt name="GroupId">
          <caption>Gruppe</caption>
        </prompt>
        <prompt name="ModifiedOn">
          <caption>Geändert am</caption>
        </prompt>
        <prompt name="ModifiedBy">
          <caption>Geändert von</caption>
        </prompt>
        <line name="Line2" />
        <prompt name="Modifiable">
          <caption>Änderbar</caption>
        </prompt>
      </controls>
    </form>
    <form name="Page1">
      <caption>Weitere Informationen</caption>
      <extendedfunctions />
      <controls>
        <groupbox name="GroupBox2">
          <caption>Definition</caption>
          <controls>
            <TextEdit name="TextEdit1">
              <tooltip>Issue Messsage if Validation fails</tooltip>
              <caption>Issue Meldung</caption>
            </TextEdit>
            <TextEdit name="Formula">
              <tooltip>Validation Formula that returns a boolean value. If true the Issue Message will appear on Validation Tab. If a string is returned the string will appear instead of the Issue Message.</tooltip>
              <caption>Formel</caption>
            </TextEdit>
            <PromptStringBrowseDefinition name="PromptStringBrowseSeverity">
              <caption>Schweregrad</caption>
              <multiselect>false</multiselect>
              <forcevalid>false</forcevalid>
            </PromptStringBrowseDefinition>
            <PromptStringBrowseDefinition name="PromptStringBrowseSection">
              <caption>Sektion</caption>
              <multiselect>false</multiselect>
              <forcevalid>false</forcevalid>
            </PromptStringBrowseDefinition>
          </controls>
        </groupbox>
        <groupbox name="GroupBox1">
          <caption>Regel</caption>
          <controls>
            <prompt name="RuleType">
              <caption>Regel Typ</caption>
            </prompt>
            <label name="RuleScope">
              <caption>Geltungsbereich</caption>
            </label>
            <radiobutton name="RadioButton1">
              <width>129</width>
              <caption>Studien Template</caption>
              <radiogroupindex>0</radiogroupindex>
              <CheckAlign>Left</CheckAlign>
            </radiobutton>
            <radiobutton name="RadioButton2">
              <caption>Studie</caption>
              <radiogroupindex>0</radiogroupindex>
              <CheckAlign>Left</CheckAlign>
            </radiobutton>
            <PromptTableNameBrowseDefinition name="PromptTableNameBrowse">
              <caption>Tabelle</caption>
              <multiselect>false</multiselect>
              <forcevalid>false</forcevalid>
            </PromptTableNameBrowseDefinition>
          </controls>
        </groupbox>
      </controls>
    </form>
  </forms>
</interface>
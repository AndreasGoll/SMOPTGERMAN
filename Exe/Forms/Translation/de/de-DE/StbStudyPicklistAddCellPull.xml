          <ShowRemoved>Default</ShowRemoved>
        </entityBrowseDefinition>
        <entityBrowseDefinition name="EntityBrowseCells">
          <ShowColumnHeaders>false</ShowColumnHeaders>
          <ExplorerColumns>
            <EntityBrowseColumnDefinition name="Name">
              <Title>Name</Title>
              <Width>75</Width>
              <Column>Name</Column>
              <HorizontalAlignment>0</HorizontalAlignment>
            </EntityBrowseColumnDefinition>
            <EntityBrowseColumnDefinition name="ItemNumber">
              <Title>Element</Title>
              <Width>75</Width>
              <Column>ItemNumber</Column>
              <HorizontalAlignment>0</HorizontalAlignment>
            </EntityBrowseColumnDefinition>
            <EntityBrowseColumnDefinition name="ParentStudy">
              <Title>Studie</Title>
              <Width>75</Width>
              <Column>ParentStudy</Column>
              <HorizontalAlignment>0</HorizontalAlignment>
            </EntityBrowseColumnDefinition>
            <EntityBrowseColumnDefinition name="PullStatus">
              <Title>Status</Title>
              <Width>75</Width>
              <Column>PullStatus</Column>
              <HorizontalAlignment>0</HorizontalAlignment>
            </EntityBrowseColumnDefinition>
            <EntityBrowseColumnDefinition name="PullDueDate">
              <Title>Pull FÃ¤lligkeit</Title>
              <Width>75</Width>
              <Column>PullDueDate</Column>
              <HorizontalAlignment>0</HorizontalAlignment>
            </EntityBrowseColumnDefinition>
          </ExplorerColumns>
          <ShowAllVersions>Default</ShowAllVersions>
          <ShowRemoved>Default</ShowRemoved>
        </entityBrowseDefinition>
      </components>
      <extendedfunctions />
      <controls>
        <explorergridcontrol name="ExplorerGridCell">
          <caption>ExplorerGridCell</caption>
        </explorergridcontrol>
        <panel name="PanelCriteria">
          <caption>Panel1</caption>
          <controls>
            <picturebox name="PictureBox">
              <caption>PictureBox1</caption>
            </picturebox>
            <label name="LabelDescription">
              <caption>Selektiere Zellen aus der Liste und klicke OK um diese in die Entnahmeliste aufzunehmen.</caption>
            </label>
            <PromptEntityBrowseDefinition name="PromptCriteria">
              <caption>Kriterien</caption>
            </PromptEntityBrowseDefinition>
          </controls>
        </panel>
        <panel name="PanelButtons">
          <caption>Panel1</caption>
          <controls>
            <ActionButtonDefinition name="ActionButtonCancel">
              <caption>&amp;Abbrechen</caption>
              <size>
                <Width>0</Width>
                <Height>0</Height>
              </size>
              <formresult>Abort</formresult>
              <accept>false</accept>
              <cancel>false</cancel>
              <save>false</save>
            </ActionButtonDefinition>
            <ActionButtonDefinition name="ActionButtonOk">
              <caption>&amp;OK</caption>
              <size>
                <Width>0</Width>
                <Height>0</Height>
              </size>
              <formresult>Abort</formresult>
              <accept>false</accept>
              <cancel>false</cancel>
              <save>false</save>
            </ActionButtonDefinition>
          </controls>
        </panel>
      </controls>
